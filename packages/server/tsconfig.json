    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "typeRoots": ["./node_modules/@types"],
    "extends": "./tsconfig.e2e.json",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "rootDir": ".",
    "skipLibCheck": true,
    "incremental": true
  }
}
